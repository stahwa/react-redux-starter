{
  "env" : {
		"browser" : true,
		"es6" : true,
		"node" : true,
		"mocha" : true
	},
  "extends": "airbnb",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "import/extensions": ["error", "never", { "packages": "always" }],
    "react/require-default-props": [0],
    "comma-dangle": ["error", "never"]
  },
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "webpack.common.js"
      }
    }
  }
}